# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /restaurants                controllers.RestaurantController.getRestaurants
GET     /getDishes/:restaurantId    controllers.RestaurantController.getDishes(restaurantId: Long)

GET     /userInfo                   controllers.CustumerController.getUser

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
